msgstr "UAPAY: Order not paid"

msgid "Заказ не оплачен"
msgstr "Order not paid"

msgid "UAPAY: Заказ успешно оплачен"
msgstr "UAPAY: Order successfully paid"

msgid "ID платежа(payment id): "
msgstr "Payment id: "

msgid "ID счета(invoice id): "
msgstr "Invoice id: "

